{
  "problemMatcher": [
    {
      "owner": "kernel-compile-docker",
      "pattern": [
        {
          "regexp": "^(?:\\+|\\-)?(?:/linux/)?(.*):(\\d+):(\\d+):\\s+(error|warning):\\s+(.*)$",
          "file": 1,
          "line": 2,
          "column": 3,
          "severity": 4,
          "message": 5
        }
      ]
    }
  ]
}

